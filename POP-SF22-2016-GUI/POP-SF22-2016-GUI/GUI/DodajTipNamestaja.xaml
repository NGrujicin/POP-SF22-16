<Window x:Class="POP_SF22_2016_GUI.GUI.DodajTipNamestaja"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF22_2016_GUI.GUI"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="DodajTipNamestaja" Height="250" Width="350">
    <Grid>
        <Label Name="lblNaziv" Content="Naziv:" Height="30" Width="50" RenderTransformOrigin="-0.347,-0.303" Margin="32,40,262,150" ></Label>
        <TextBox Name="tbNaziv" Margin="97,40,40,144" Text="{Binding Path=Naziv}" />
        <Button Click="btnSacuvaj_Click" Name="btnSacuvaj" Content="Sacuvaj" Margin="32,138,207,45" />
        <Button Click="btnNazad_Click" Name="btnNazad" Content="Nazad" Margin="197,138,40,45" />
    </Grid>
</Window>
